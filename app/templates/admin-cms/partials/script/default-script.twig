</script>

<script>

    String.prototype.format = function()
    {
        var content = this;
        for (var i=0; i < arguments.length; i++)
        {
            var replacement = '{' + i + '}';
            content = content.replace(replacement, arguments[i]);
        }
        return content;
    };

    function resetForm(idForm) {
        document.getElementById(idForm).reset();
    }
</script>